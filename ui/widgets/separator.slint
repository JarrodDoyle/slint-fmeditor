export component VerticalSeparator {
    Path {
        width: 1px;
        stroke: black;
        stroke-width: 1px;
        commands: "M 0 0 L 0 1 Z";
    }
}

export component HorizontalSeparator {
    Path {
        height: 1px;
        stroke: black;
        stroke-width: 1px;
        commands: "M 0 0 L 1 0 Z";
    }
}
